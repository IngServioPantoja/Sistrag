  background-color rgba(255,255,255,0.9)
  border-radius 10px
  box-shadow 0px 0px 15px 10px rgba(255,255,255,0.9)
  -moz-box-shadow 0px 0px 15px 10px rgba(255,255,255,0.9)
  -webkit-box-shadow 0px 0px 15px 10px rgba(255,255,255,0.9)
  display none
  left 0px
  padding 10px
  position absolute
  right 0px
  top 80px
  width 120px
  z-index 2

  #circular3dG
    position relative
    width 80px
    height 80px


    .circular3dG
      position absolute
      background-color #040075
      width 23px
      height 23px
      -moz-border-radius 24px
      -moz-animation-name bounce_circular3dG
      -moz-animation-duration 0.96s
      -moz-animation-iteration-count infinite
      -moz-animation-direction linear
