@font-face {
  font-family: 'icomoon'; 
  src: url('fonts/icomoon.eot'); 
  src: url('fonts/icomoon.eot?#iefix') format('embedded-opentype'),
    url('fonts/icomoon.woff') format('woff'),
    url('fonts/icomoon.ttf') format('truetype'),
    url('fonts/icomoon.svg#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

::-webkit-scrollbar {
    width: 5px;
}
::-moz-scrollbar {
    -moz-width: 5px;
}
   
::-webkit-scrollbar-track {
  -webkit-box-shadow: #000000;
}
::-moz-scrollbar-track {
  -moz-box-shadow: #000000;
}

::-webkit-scrollbar-button 
{
  padding-bottom: 10px;

}
::-moz-scrollbar-button 
{
  -moz-padding-bottom: 10px;

}

::-webkit-scrollbar-track-piece 
{
  background: #000000;  
  -webkit-box-shadow: inset 0 0 5px rgba(31,87,148,0.4); 

}
::-moz-scrollbar-track-piece 
{
  -moz-background: #000000; 
  -moz-box-shadow: inset 0 0 5px rgba(31,87,148,0.4); 

}
   
::-webkit-scrollbar-thumb {
  background: #000000;
  -webkit-box-shadow: inset 0 0 15px rgba(31,87,148,1); 
      transition:all 0.3s linear;
    -webkit-transition:all 0.3s linear;
    -moz-transition:all 0.3s linear;

}
::-moz-scrollbar-thumb {
  -moz-background: #000000;
  -moz--box-shadow: inset 0 0 10px rgba(31,87,148,1); 
  -moz-transition:all 0.3s linear;
    -moz--transition:all 0.3s linear;
    -moz-transition:all 0.3s linear;

}

::-webkit-scrollbar-thumb:hover {
  background: #000000;
  -webkit-box-shadow: inset 0 0 10px rgba(159,255,253,1); 
}
::-moz-scrollbar-thumb:hover {
  -moz-background: #000000;
  -moz-box-shadow: inset 0 0 10px rgba(159,255,253,1); 
}

