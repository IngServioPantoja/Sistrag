    -webkit-transform:scale(1)
  100%
    -webkit-transform:scale(.3)

@-o-keyframes bounce_circular3dG
  0%
    -o-transform:scale(1)
  100%
    -o-transform:scale(.3)

@-ms-keyframes bounce_circular3dG
  0%
    -ms-transform:scale(1)
  100%
    -ms-transform:scale(.3)

